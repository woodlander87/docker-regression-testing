{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist",
    "inlineSourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "e2e",
    "node_modules",
    "resources"
  ]
}
